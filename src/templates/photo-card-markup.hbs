{{#each this}}
<div class="photo-card">
  <img class='image'src="{{webformatURL}}" alt="{{tags}}" data-id="{{id}}"'/>

  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      {{likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      {{views}}
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{downloads}}
    </p>
    <div class='image-big-place' hidden>
      <img class='image-big'src="{{largeImageURL}}" alt="{{tags}}" data-2id="{{id}}" />
    </div>
    
  </div>
</div>
{{/each}}